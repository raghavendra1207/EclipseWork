package demo;

import java.lang.reflect.*;
import com.entity.*;
public class ReflectDemo {
	public static void main(String args[]) {
		Method[] new1=PrivateDeclaration.class.getDeclaredMethods();
		Field[] n2=PrivateDeclaration.class.getFields();
		for(Method n:new1) {
			System.out.println(n);
			
		}
		for(Field n:n2) {
			System.out.println(n);	
			}
		
	}

}
